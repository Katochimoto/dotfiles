#!/usr/bin/perl
$dic = @ARGV[0];
$xsl = @ARGV[1];

open (DIC, @ARGV[0]) || die "can't open dic";
open (XSL, @ARGV[1]) || die "can't open xsl";
@xsl = <XSL>;
close XSL;

while ($dic_line = <DIC>) {
    @dic_line = split /\s/, $dic_line;
    $id = @dic_line[0];
    $used = 0;

    foreach $xsl_line (@xsl){
        if (index($xsl_line, $id) > -1) {
            $used++;
        }
    }

    if (!$used) {
        print $id;
    }
}
